{
  "name": "@progress/kendo-angular-layout",
  "version": "7.1.5",
  "description": "Kendo UI for Angular Layout Package - a collection of components to create professional application layoyts",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Progress",
  "homepage": "https://www.telerik.com/kendo-angular-ui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-angular-layout.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "keywords": [
    "Angular",
    "Kendo UI",
    "PanelBar",
    "Progress",
    "TabStrip",
    "angular splitter",
    "angular templates",
    "angular-layouts",
    "angularjs",
    "dashboardlayout",
    "drawer",
    "expansionpanel",
    "panelbar",
    "split-container",
    "split-layout",
    "split-panes",
    "splitter",
    "stepper",
    "tabstrip",
    "tilelayout"
  ],
  "@progress": {
    "friendlyName": "Layout"
  },
  "dependencies": {
    "@progress/kendo-schematics": "^3.0.0",
    "@progress/kendo-draggable": "^3.0.2",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@angular/animations": "12 - 14",
    "@angular/common": "12 - 14",
    "@angular/core": "12 - 14",
    "@progress/kendo-angular-common": "^3.0.0",
    "@progress/kendo-angular-l10n": "^4.0.0",
    "@progress/kendo-angular-progressbar": "^3.0.0",
    "@progress/kendo-licensing": "^1.0.2",
    "rxjs": "^6.5.3 || ^7.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  },
  "schematics": "./schematics/collection.json",
  "release": {
    "debug": false,
    "branchTags": {
      "develop": "dev"
    },
    "fallbackTags": {
      "dev": "latest"
    },
    "analyzeCommits": "@progress/semantic-prerelease/analyzeCommits",
    "generateNotes": "@progress/kendo-angular-tasks/lib/generateNotes",
    "getLastRelease": "@progress/semantic-prerelease/getLastRelease",
    "verifyConditions": "@progress/semantic-prerelease/verifyConditions",
    "verifyRelease": "@progress/semantic-prerelease/verifyRelease"
  },
  "main": "bundles/kendo-angular-layout.umd.js",
  "module": "fesm2015/kendo-angular-layout.js",
  "es2015": "fesm2015/kendo-angular-layout.js",
  "esm2015": "esm2015/kendo-angular-layout.js",
  "fesm2015": "fesm2015/kendo-angular-layout.js",
  "typings": "kendo-angular-layout.d.ts",
  "sideEffects": false
}