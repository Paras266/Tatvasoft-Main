{
  "name": "@progress/kendo-angular-l10n",
  "version": "4.0.0",
  "description": "Localization Services - Kendo UI Angular",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Progress",
  "homepage": "https://www.telerik.com/kendo-angular-ui/components/",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-angular-l10n.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "keywords": [
    "Angular",
    "Kendo UI"
  ],
  "@progress": {
    "friendlyName": "Localization"
  },
  "dependencies": {
    "tslib": "^2.3.1",
    "@progress/kendo-schematics": "^3.0.0"
  },
  "peerDependencies": {
    "@angular/common": "12 - 14",
    "@angular/core": "12 - 14",
    "rxjs": "^6.5.3 || ^7.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  },
  "schematics": "./schematics/collection.json",
  "release": {
    "debug": false,
    "branchTags": {
      "develop": "dev"
    },
    "fallbackTags": {
      "dev": "latest"
    },
    "analyzeCommits": "@telerik/semantic-prerelease/analyzeCommits",
    "generateNotes": "@telerik/semantic-prerelease/generateNotes",
    "getLastRelease": "@telerik/semantic-prerelease/getLastRelease",
    "verifyConditions": "@telerik/semantic-prerelease/verifyConditions",
    "verifyRelease": "@telerik/semantic-prerelease/verifyRelease"
  },
  "main": "bundles/kendo-angular-l10n.umd.js",
  "module": "fesm2015/kendo-angular-l10n.js",
  "es2015": "fesm2015/kendo-angular-l10n.js",
  "esm2015": "esm2015/kendo-angular-l10n.js",
  "fesm2015": "fesm2015/kendo-angular-l10n.js",
  "typings": "kendo-angular-l10n.d.ts",
  "sideEffects": false
}